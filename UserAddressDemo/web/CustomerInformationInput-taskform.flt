                            		<div class="div_texbox">
                              		<input name="enteredNickName" type="text" class="textbox" id="enteredNickName" value="" />
                            		</div>	
                            
              <div class="button_div">
                <input name="Submit" type="submit" value="Submit" class="buttons" />
              </div>
            </form>
            <div class="clear"></div>
          </fieldset>
	</div>
	<div id="footer">
	</div>
